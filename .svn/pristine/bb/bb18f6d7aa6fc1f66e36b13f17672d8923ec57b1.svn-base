#SnapperOverlay {
    display: none;
    position: absolute;
    width: auto;
    height: auto;
    background-color: gray;
    top: 70px;
    right: 0;
    bottom: 0;
    left: 0;
    opacity: 0.8;
}

.custom-file-input::-webkit-file-upload-button {
    visibility: hidden;
}
.custom-file-input {
    position: relative;
    color:#696969 !important;
    border-color:#bbbbbb !important;
    display: inline-block;
    white-space: nowrap;
    -webkit-user-select: none;
    cursor: pointer;
    font-size:14px;
    font-weight:bold;
    color:#ffffff;
}

.custom-file-input input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    width: 100%;
    height: 100%;

    filter: alpha(opacity=0);
    opacity: 0;
    outline: none;
    background: white;
    cursor: inherit;
    display: block;
}