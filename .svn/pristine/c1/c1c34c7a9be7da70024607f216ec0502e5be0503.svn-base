    float: left;
    margin: 10px;
}

/* collapse adjacent horizontal margins */
.column+.column {
    margin-left: 0px;
}

.columntitle {
    margin: 0px 10px;
    font-size: 16px;
    font-weight: bold;
    color: #656565;
}


.columnbody {
    padding-left: 7px;
    padding-top: 10px;
    font-size: 14px;
}

.recorderinstructions1 li {
    margin-bottom: 1em;
    font-size: 12px;
}

#profileName{
    width:131px;
    height:20px;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}