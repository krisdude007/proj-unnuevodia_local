    float: right !important;
    margin-left: 10px;
    margin-right: 0px;
}

.fab-tile .fab-tile-body .content {
    display: inline-block;
}

.fab-tile .fab-tile-body > i {
    margin-top: 17px;
    display: block;
    font-size: 56px;
    text-align: center;
}

