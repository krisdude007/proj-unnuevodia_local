/*********************
 GENERAL RESET & SETUP 
*********************/

/***
Import fonts
***/
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700);

/***
Reset and overrides  
***/
/* general body settings */
body { 
    color: #000; 
    font-family: 'Open Sans';
    padding: 0px !important;
    margin: 0px !important;
    font-size:12px; 
}

/***
General typography 
***/
h3 small, h4 small, h5 small {
    color: #444;
}

h1, h2, h3, h4, h5, h6 {
    font-weight: 300;
}

h1.block, h2.block, h3.block, h4.block, h5.block, h6.block {
    padding-bottom: 10px;
}

a {
    text-shadow: none;
    color: #0d638f;
}

/***
General backgrounds
***/
.bg-blue {
    background-image: none !important;
    background-color: #4b8df8 !important;
}

.bg-red {
    background-image: none !important;
    background-color: #e02222 !important;
}

.bg-yellow {
    background-image: none !important;
    background-color: #ffb848 !important;
}

.bg-green {
    background-image: none !important;
    background-color: #35aa47 !important;
}

.bg-purple {
    background-image: none !important;
    background-color: #852b99 !important;
}

.bg-grey {
    background-image: none !important;
    background-color: #555555 !important;
}

/***
Apply fix for font awesome icons.
***/
[class^="icon-"], 
[class*=" icon-"],
[class^="icon-"]:hover, 
[class*=" icon-"]:hover { 
    background: none !important;
}

/***
Close icon used for modal dialog and other UI element close buttons
***/
.close {
    display: inline-block;
    margin-top: 0px;
    margin-right: 0px;
    width: 9px;
    height: 9px;
    background-repeat: no-repeat !important;
    background-image: url("/core/webassets/images/remove-icon-small.png");
}

/***
Fix link outlines after click
***/
a,a:focus, a:hover, a:active {
    outline: 0;
}

.sort-link{
    color:white;
}

table th a.asc{
    background: url('/core/webassets/images/up.gif') right center no-repeat;
    padding-right: 10px;
    color:#DEDEDE;
}

table th a.desc{
    background: url('/core/webassets/images/down.gif') right center no-repeat;
    padding-right: 10px;
    color:#DEDEDE;
}

table th a.asc:hover,table th a.desc:hover{
    color:white;
}

/***
IE8 fix for form input height in fluid rows
***/
.ie8 .fab-row-fluid [class*="span"] {
    min-height: 20px !important;
}

/***
Fix grid offset used for reponsive layout handling(refer app.js=>handleResponsive)
***/
.fix-offset {
    margin-left: 0px !important;
}

/***
Misc tools
***/
.visible-ie8 {
    display: none
}

.no-padding {
    padding: 0px !important;
}

.no-margin {
    margin: 0px !important;
}

.no-bottom-space {
    padding-bottom:0px !important;
    margin-bottom: 0px !important;
}

.no-top-space {
    padding-top:0px !important;
    margin-top: 0px !important;
}

.space5 {
    display: block;
    height: 5px !important;
    clear: both;
}

.space7 {
    height: 7px !important;
    clear: both;
}

.space10 {
    height: 10px !important;
    clear: both;
}

.space12 {
    height: 12px !important;
    clear: both;
}

.space15 {
    height: 15px !important;
    clear: both;
}

.space20 {
    height: 20px !important;
    clear: both;
}

.no-space {
    margin: 0px !important;
    padding: 0px !important;
}

.no-text-shadow {
    text-shadow: none !important;
}

.margin-bottom-10 {
    margin-bottom: 10px !important;
}

.fab-hide {
    display: none;
}

.bold {
    font-weight:400;
}

.fix-margin {
    margin-left: 0px !important
}

.border {
    border: 1px solid #ddd
}

hr {
    margin: 20px 0;
    border: 0;
    border-top: 1px solid #E0DFDF;
    border-bottom: 1px solid #FEFEFE;
}

.floatLeft{
    float:left;
}
.floatRight{
    float:right;
}
html[dir='rtl'] .floatLeft{
    float:right;
}
html[dir='rtl'] .floatRight{
    float:left;
}
.marginRight10{
    margin-right: 10px;
}
html[dir='rtl'] .marginRight10{
    margin-left: 10px;
}

/********************
 GENERAL LAYOUT 
*********************/

/***
Header and header elements.
***/
.fab-header {
    padding: 0 !important;
    margin: 0 !important;
}

.fab-header .brand {
    margin-top: -1px;
}

.fab-header .fab-btn-navbar {
    margin-bottom: 0px;
    padding-right: 0px;
    padding-top:10px;
    padding-bottom: 6px; 
    background-image: none;
    filter:none;
    box-shadow: none;
    color: #fff;
    border: 0;
}

.fab-header .fab-btn-navbar:hover {
    text-decoration: none;
}

.fab-header .fab-navbar-inner {
    width: 100%;
    margin-left: 0;
    border: 0px;
    padding: 0px; 
    box-shadow: none;
}

.fab-header .fab-nav {
    display: block; 
}

.fab-header .fab-nav > li {
    margin: 0px;
    padding: 0px;
}

.fab-header .fab-nav > li.fab-dropdown, 
.fab-header .fab-nav > li.fab-dropdown > a {
    padding-left: 4px; 
    padding-right: 4px;
}

.fab-header .fab-nav > li.fab-dropdown:last-child {
    padding-right: 2px;
}

.fab-header .fab-nav > li.fab-dropdown .fab-dropdown-toggle {
    margin: 0px;
    padding: 14px 2px 8px 2px;
}

.fab-header .fab-nav > li.fab-dropdown .fab-dropdown-toggle i {
    font-size: 18px;
}

.fab-header .fab-nav > li.fab-dropdown.fab-user .fab-dropdown-toggle {
    padding: 6px 4px 7px 9px;
}

.fab-header .fab-nav > li.fab-dropdown.fab-user .fab-dropdown-toggle:hover {
    text-decoration: none;
}

.fab-header .fab-nav > li.fab-dropdown.fab-user .fab-dropdown-toggle .fab-username {
    color: #ddd;
}

.fab-header .fab-nav li.fab-dropdown.fab-user .fab-dropdown-toggle i {
    display: inline-block;
    margin-top: 5px;
    margin: 0;
    font-size: 16px;
}

.fab-header .fab-nav > li.fab-dropdown .fab-dropdown-toggle .badge {
    position: absolute;
    font-size: 11px !important;
    font-weight: 300;
    top: 8px;
    right: 24px;
    text-align: center;
    height: 14px;
    background-color: #e02222;
    padding: 2px 6px 2px 6px;
    -webkit-border-radius: 12px !important;
    -moz-border-radius: 12px !important;
    border-radius: 12px !important;
    text-shadow:none !important;
}

/* firefox hack for top bar badges */
@-moz-document url-prefix() { 
    .fab-header .fab-nav li.fab-dropdown .fab-dropdown-toggle .badge {
    padding: 1px 6px 3px 6px;
}
}

.fab-header .fab-nav .fab-dropdown-menu {
    margin-top: 3px;
}

/***
Page container
***/
.fab-page-container {
    margin: 0px;
    padding: 0px;
    width:100% !important;
}

.fab-fixed-top .fab-page-container {
    margin-top: 55px; 
}

/***
Page sidebar
***/
.fab-page-sidebar > ul {
    list-style: none;
    margin: 0;
    padding: 0;
    margin: 0;
    padding: 0; 
}

.fab-page-sidebar > ul > li {
    display: block;
    margin: 0;
    padding: 0; 
    border: 0px;
}

.fab-page-sidebar > ul > li.fab-start > a {
    border-top-color: transparent !important;
}

.fab-page-sidebar > ul > li:last-child > a {
    border-bottom: 1px solid transparent !important;
}

.fab-page-sidebar > ul > li > a {
    display: block;
    position: relative;
    margin: 0;
    border: 0px;
    padding: 10px 15px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 300;
    color:white;
}

.fab-page-sidebar > ul > li > a i {
    font-size: 16px;
    margin-right: 5px;
    text-shadow:none; 
}

.fab-page-sidebar > ul > li.fab-active > a {
    border: none; 
    text-shadow:none;
}  

.fab-page-sidebar > ul > li.fab-active > a .fab-selected {
    display: block;
    width: 8px;
    height: 25px;
    background-image: url("../images/sidebar-menu-arrow.png");
    float: right;
    position: absolute;
    right:0px;
    top:8px;
}

.fab-page-sidebar ul > li > a .fab-arrow:before {  
    float: right;
    margin-top: 1px;
    margin-right: 5px;
    display: inline;
    font-size: 16px;
    font-family: FontAwesome;
    height: auto;
    content: "\f104";
    font-weight: 300;
    text-shadow:none;
}

.fab-page-sidebar > ul > li > a .fab-arrow.open:before {   
    float: right;
    margin-top: 1px;
    margin-right: 5px;
    display: inline;
    font-family: FontAwesome;
    height: auto;
    font-size: 16px;
    content: "\f107";
    font-weight: 300;
    text-shadow:none;
}

.fab-page-sidebar > ul > li > ul.fab-sub {
    display: none;
    list-style: none;
    clear: both;
    margin: 8px 0px 8px 0px;
}

.fab-page-sidebar > ul > li.fab-active > ul.fab-sub {
    display: block;
}

.fab-page-sidebar > ul > li > ul.fab-sub > li {
    background: none;
    margin: 0px;
    padding: 0px;
    margin-top: 1px !important;
}

.fab-page-sidebar > ul > li > ul.fab-sub > li > a {
    display: block;
    margin: 0px 0px 0px 0px;
    padding: 5px 0px;
    padding-left: 44px !important;
    color: #ccc;
    text-decoration: none;
    text-shadow: 0 1px 1px #000;
    font-size: 14px;
    font-weight: 300;
    background: none;
}

.fab-page-sidebar > ul > li > ul.fab-sub > li > a > i {
    font-size: 13px;
}

.fab-page-sidebar .sidebar-search {
    margin: 8px 20px 20px 20px;
}

.fab-page-sidebar .sidebar-search .submit {
    display: block;
    float: right;
    margin-top: 8px;
    width: 13px;
    height: 13px;
    background-image: url(../images/search-icon.png);
    background-repeat: no-repeat;
}

.fab-page-sidebar .sidebar-search input {
    margin: 0px;
    width: 165px;
    border: 0px; 
    padding-left: 0px;
    padding-right: 0px;
    padding-bottom: 0px;
    font-size: 14px ;
    box-shadow: none;
}

.fab-page-sidebar .sidebar-search .input-box {
    padding-bottom: 2px;
    border-bottom:1px solid #959595;
}

/***
Sidebar toggler(show/hide)
***/

.fab-sidebar-toggler {
    cursor: pointer; 
    opacity: 0.5;
    filter: alpha(opacity=50);
    margin-top: 15px;
    margin-left: 175px;
    width: 29px;
    height: 29px;
    background-repeat: no-repeat;
}

.fab-sidebar-toggler:hover { 
    filter: alpha(opacity=100);
    opacity: 1;
}

.sidebar-closed .fab-sidebar-toggler {  
    margin-left: 3px;
}

.sidebar-closed .fab-page-sidebar .sidebar-search {  
    height: 34px;    
    width: 29px;
    margin-left: 3px;  
    margin-bottom: 0px;
}

.sidebar-closed .fab-page-sidebar .sidebar-search input {
    display: none;
}

.sidebar-closed .fab-page-sidebar .sidebar-search .submit { 
    margin: 11px 7px !important;
    display: block !important;
}

.sidebar-closed .fab-page-sidebar .sidebar-search .input-box {
    border-bottom: 0;
}

.sidebar-closed .fab-page-sidebar .sidebar-search.open {  
    height: 34px;    
    width: 255px;
    overflow: hidden;
}

.sidebar-closed .fab-page-sidebar .sidebar-search.open input {  
    margin-top: 3px;
    padding-left: 10px;
    padding-bottom: 2px;
    width: 180px;
    display: inline-block !important;
}

.sidebar-closed .fab-page-sidebar .sidebar-search.open .submit {
    display: inline-block;
    width: 13px;
    height: 13px;
    margin: 10px 8px 9px 6px !important;
}

.sidebar-closed .fab-page-sidebar .sidebar-search.open .fab-remove {
    background-repeat: no-repeat;
    width: 11px;
    height: 11px;
    margin: 11px 6px 7px 8px !important;
    display: inline-block !important;
    float: left !important;
}

.sidebar-closed ul > li > a .fab-selected {
    right: -3px !important;
}

.sidebar-closed ul > li > a .fab-title,
.sidebar-closed ul > li > a .fab-arrow {
    display: none !important;
}

.sidebar-closed .fab-sidebar-toggler {
    margin-right: 3px;
}

.sidebar-closed .fab-page-sidebar .sidebar-search {
    margin-top: 6px;
    margin-bottom: 6px;
}

.sidebar-closed > .fab-page-sidebar > ul {
    width: 35px !important;
}

.sidebar-closed .fab-page-sidebar > ul > li > a {
    padding-left: 7px;
}

.sidebar-closed .fab-page-sidebar > ul > li:hover {
    width: 225px;   
    position: relative;
    z-index: 2000;
    display: block !important;
}

.sidebar-closed .fab-page-sidebar > ul > li:hover .fab-selected {
    display: none;
}

.sidebar-closed .fab-page-sidebar > ul > li:hover > a > i {
    margin-right: 10px;
}

.sidebar-closed .fab-page-sidebar > ul > li:hover .fab-title {
    display: inline !important;
}

.sidebar-closed .fab-page-sidebar > ul > li.fab-has-sub .fab-sub {
    display: none !important;
}

.sidebar-closed .fab-page-sidebar > ul > li.fab-has-sub:hover .fab-sub {  
    width: 189px;
    position: absolute;
    z-index: 2000;
    left: 36px;
    margin-top: 0;
    display: block !important;
}

.sidebar-closed .fab-page-sidebar > ul > li.fab-has-sub:hover .fab-sub > li > a {
    padding-left: 15px !important;
}

.sidebar-closed .fab-page-sidebar {
    width: 35px;
}

.sidebar-closed .fab-page-content {
    margin-left: 35px !important;
}

/* ie8 fixes */
.ie8 .fab-page-sidebar {
    position: absolute;
    width: 225px;
}

.ie8 .fab-page-sidebar ul{
    width: 225px;
}

/***
Page content
***/
.fab-page-content {  
    margin-top: 0px;   
    padding: 0px;
    background-color: #fff; 
}

.ie8 .fab-page-content { 
    margin-left: 225px; 
    margin-top: 0px;
}

/***
Page title
***/
.page-title {
    padding: 0px;
    font-size: 30px;
    letter-spacing: -1px;
    display: block;
    color: #666;
    margin: 20px 0px 15px 0px;
    font-weight: 300;
    font-family: 'Open Sans';
}

.page-title small {
    font-size: 14px;
    letter-spacing: 0px;
    font-weight: 300;
    color: #888;
}

/***
Page breadcrumb
***/
.fab-page-content .breadcrumb {
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
    border-radius: 0px;
    box-shadow: none;
    padding-right: 0px;
    padding-left: 8px;
    margin-bottom: 25px;
    border:0px !important;  
    background-color: #eee;
}

.fab-page-content .breadcrumb a, 
.fab-page-content .breadcrumb i, 
.fab-page-content .breadcrumb span {
    color: #333;
    font-size: 14px;
    text-shadow:none;
}

.fab-page-content .breadcrumb i {
    color: #666;
}

/***
Dashboard date range panel
***/
.fab-page-content .breadcrumb .dashboard-date-range  {
    display: none;
    padding-top: -1px;
    margin-right: 0px;
    margin-top: -8px;
    padding: 8px;
    padding-bottom: 7px;
    cursor: pointer;
    min-width: 275px;
    color: #fff;
    background-color: #e02222;
}

.fab-page-content .breadcrumb .dashboard-date-range span {
    font-size: 12px;
    font-weight: 300; 
    color: #fff;
    text-transform: uppercase;
} 

.fab-page-content .breadcrumb .dashboard-date-range .icon-calendar {
    text-transform: none;
    color: #fff;
    margin-top: 0px;
    font-size: 14px;
}
.fab-page-content .breadcrumb .dashboard-date-range span {
    font-weight: normal; 
} 
.fab-page-content .breadcrumb .dashboard-date-range .icon-angle-down {
    color:#fff;
    font-size: 16px;
}

/***
Footer
***/
.fab-footer {
    padding: 8px 20px 5px 20px; 
    font-size: 12px;
    color: #999;
}

.fab-footer .go-top { 
    display: block;
    text-decoration: none;
    color: #fff;
    cursor: pointer;
    margin-top: -2px;
    margin-right: 0px;
    margin-bottom: 0px;
    font-size: 16px;
    background-color: #111;
    padding: 0px 6px 0px 6px;
}

.fab-footer .go-top i {
    color: #666;
    font-size: 22px;
    margin-bottom: 5px; 
}

/********************
 GENERAL UI ELEMENTS 
*********************/

/***
Icon stuff
***/
i.icon, a.icon {
    color: #999;
    margin-right: 5px;
    font-weight: normal;
    font-size: 13px;
}

i.icon-black {
    color: #000 !important;
}

a.icon:hover {
    text-decoration: none;
    -webkit-transition: all 0.1s ease-in-out;
    -moz-transition: all 0.1s ease-in-out;
    -o-transition: all 0.1s ease-in-out;
    -ms-transition: all 0.1s ease-in-out;
    transition: all 0.1s ease-in-out;
    opacity: .4;
    filter:alpha(opacity=40);
}

a.icon.fab-huge i{
    font-size: 16px !important;
}

i.big {
    font-size: 20px;
}

i.warning {
    color: #d12610;
}

i.critical {
    color: #37b7f3;
}

i.normal {
    color: #52e136;
}

/***
Custom wells
***/
.fab-well {
    background-color: #fafafa;
    border: 1px solid #ddd;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
    border-radius: 0px;   
    -webkit-box-shadow: none !important;
    -moz-box-shadow: none !important;
    box-shadow: none !important;        
}

.fab-well.mini {
    padding: 7px !important;
}

/***
Form stuff
***/
form legend {
    margin: 15px 0px 10px 0px !important;
}

.fab-form form {
    margin: 0px !important;
    padding: 0px !important;
}

.no-bottom-space {
    padding-bottom: 0px !important;
    margin-bottom: 0px !important;
}

.fab-form .form-actions{
    margin-top: 0px !important;
    margin-top: 40px;
    padding-left: 190px;
}

.fab-portlet.fab-box .fab-form .form-actions,
.fab-portlet.fab-solid .fab-form .form-actions {
    margin-left:-10px !important;
    margin-right:-10px !important;
    margin-bottom: -10px !important;
} 

/***
Bordered form layout
***/

/***
Input icons
***/
/* input with right aligned and colored icons */
.fab-input-icon input {
    padding-right: 25px !important;
}

.fab-input-icon .input-info,
.fab-input-icon .input-error, 
.fab-input-icon .input-warning, 
.fab-input-icon .input-success {
    display: inline-block !important;
    position: relative !important;
    top: 7px;
    right: 25px !important;
    font-size: 16px;
}

.fab-input-icon .input-info {
    color:#27a9e3;
}
.fab-input-icon .input-error {
    color:#B94A48;
}
.fab-input-icon .input-warning {
    color: #C09853;
}
.fab-input-icon .input-success {
    color: #468847;
}

/* input with left aligned icons */
.fab-input-icon.fab-left i {
    color: #ccc;
    display: block !important;
    position: absolute !important;
    z-index: 1;
    margin: 9px 2px 4px 10px; 
    width: 16px;
    height: 16px;
    border: 1px solid #ddd;
    font-size: 16px;
    text-align: center;
}

.fab-input-icon.fab-left input {
    padding-left: 33px !important;
}

/***
Portlets
***/
.fab-portlet  {
    clear: both;
    margin-top: 0px;
    margin-bottom: 25px;
    padding: 0px;
}

.fab-portlet:after, .fab-portlet:before {
    display: table;
}

.fab-portlet-title {  
    margin-bottom: 15px;
    border-bottom: 1px solid #eee;
}

.fab-portlet-title h4 {
    display: inline-block;
    font-size: 16px;
    font-weight: 400;
    margin: 0;
    padding: 0;
    margin-bottom: 7px; 
}

.fab-portlet-title h4 i {
    font-size: 13px;
    margin-right: 5px;
    color: #666;
}

.fab-portlet.fab-blue .fab-portlet-title h4, .fab-portlet-title.fab-blue h4, 
.fab-portlet.fab-green .fab-portlet-title h4, .fab-portlet-title.fab-green h4,
.fab-portlet.fab-yellow .fab-portlet-title h4, .fab-portlet-title.fab-yellow h4,
.fab-portlet.fab-red .fab-portlet-title h4, .fab-portlet-title.fab-red h4,
.fab-portlet.fab-purple .fab-portlet-title h4, .fab-portlet-title.fab-purple h4,
.fab-portlet.dark-grey .fab-portlet-title h4, .fab-portlet-title.dark-grey h4 {
    color: #fff;
}

.fab-portlet.fab-blue .fab-portlet-title h4 > i, 
.fab-portlet.fab-green .fab-portlet-title h4 > i,
.fab-portlet.fab-grey .fab-portlet-title h4 > i,
.fab-portlet.fab-yellow .fab-portlet-title h4 > i, 
.fab-portlet.fab-red .fab-portlet-title h4 > i,  
.fab-portlet.fab-purple .fab-portlet-title h4 > i, 
.fab-portlet.dark-grey .fab-portlet-title h4 > i{
