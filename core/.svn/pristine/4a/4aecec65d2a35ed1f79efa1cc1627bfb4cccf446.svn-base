.fab-onoffswitch-inner {
    width: 200%; margin-left: -100%;
    -moz-transition: margin 0.1s ease-in 0s; -webkit-transition: margin 0.1s ease-in 0s;
    -o-transition: margin 0.1s ease-in 0s; transition: margin 0.1s ease-in 0s;
}

.fab-onoffswitch-inner:before, .fab-onoffswitch-inner:after {
    float: left; width: 50%; height: 24px; padding: 0; line-height: 24px;
    font-size: 14px; color: white;font-weight: bold;
    -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}

.fab-onoffswitch-inner:before {
    content: "ON";
    padding-left: 10px;
    padding-right:2px;
    background-color: #42AB32; color: #FFFFFF;
    border-right: 2px solid #42AB32;
}

.fab-onoffswitch-inner:after {
    content: "OFF\0000a0\0000a0\0000a0\0000a0";
    padding-right: 10px;
    background-color: #EEEEEE; color: #999999;
    text-align: right;
}

