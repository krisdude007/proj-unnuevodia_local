/*header*/
#fab-logo {
    height      : 24px;
    /*width       : 75px;*/
    margin-top  : 12px;
}
a:hover{
    text-decoration: none;
    color:inherit;
}
.fab-left {
    float : left;
}

html[dir='rtl'] .fab-left{
    float : right;
}


.fab-navbar-inner{
    background: url("/core/webassets/images/header.png") repeat-x scroll 0 0 transparent !important;
    height: 55px;
    margin-left: 0;
    margin-top: 0;
    padding: 0;
    width: 100%;
}

.fab-search-dropdown {
    background-image: url("/core/webassets/images/search-icon.png") !important;
    background-repeat: no-repeat !important;
    display: inline-block;
    height: 27px;
    width: 29px;
}
.fab-seach-submit {
    background-image: url("/core/webassets/images/search-button.png");
    background-repeat: no-repeat;
    border: 0 none;
    height: 19px;
    margin-top: 5px;
    width: 17px !important;
}
.fab-search-input {
    border: 0 none !important;
    float: left;
    width: 140px !important;
}
.fab-dropdown-menu {
    width: 178px;
    height: 40px;
}

.fab-notifications {
    background-image: url("/core/webassets/images/notifications-icon.png") !important;
    background-repeat: no-repeat !important;
    display: inline-block;
    height: 27px;
    width: 29px;
}
.fab-dropdown-menu li{
    background:white;
}
.fab-dropdown-menu .fab-divider{
    margin:0px;
}
.fab-user{margin-top:6px !important;}
/*end header*/

