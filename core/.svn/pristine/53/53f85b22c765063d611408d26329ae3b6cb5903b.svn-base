
/* this makes it possible to add next button beside amp_scrollable */
.amp_scrollable {
    float:left;
}

#mediaplayer_wrapper {
  width: 150px;
  height: 85px;
}

/* prev, next, prevPage and nextPage buttons */
a.browse {
    background:url(/core/webassets/images/hori_large.png) no-repeat;
    display:block;
    width:30px;
    height:30px;
    float:left;
    margin:10px 10px;
    cursor:pointer;
    font-size:1px;
}

/* right */
a.right { background-position: 0 -30px; clear:right; margin-right: 0px;}
a.right:hover { background-position:-30px -30px; }
a.right:active { background-position:-60px -30px; }


/* left */
a.left { margin-left: 0px; }
a.left:hover  { background-position:-30px 0; }
a.left:active { background-position:-60px 0; }

/* up and down */
a.up, a.down  {
    background:url(/core/webassets/images/vert_large.png) no-repeat;
    float: none;
    margin: 10px 50px;
}

/* up */
a.up:hover { background-position:-30px 0; }
a.up:active { background-position:-60px 0; }

/* down */
a.down { background-position: 0 -30px; }
a.down:hover { background-position:-30px -30px; }
a.down:active { background-position:-60px -30px; }


/* disabled navigational button */
a.disabled {
    visibility:hidden !important;
}

/*
  root element for the scrollable.
  when scrolling occurs this element stays still.
  */
.amp_scrollable {

    /* required settings */
    position:relative;
    overflow:hidden;
    width: 330px;
    height:45px;

    /* custom decorations */
    border:1px solid #ccc;
    background:url(/core/webassets/images/h300.png) repeat-x;
}

/*
   root element for amp_scrollable items. Must be absolutely positioned
   and it should have a extremely large width to accomodate amp_scrollable
   items.  it's enough that you set the width and height for the root
   element and not for this element.
*/
.amp_scrollable .items {
    /* this cannot be too large */
    width:20000em;
    position:absolute;
    clear:both;
}

.items div {
    float:left;
    width:680px;
}

/* single amp_scrollable item */
.amp_scrollable img {
    float:left;
    margin:5px 5px 5px 5px;
    background-color:#fff;
    padding:2px;
    border:1px solid #ccc;
    width:50px;
    height:28px;

    -moz-border-radius:4px;
    -webkit-border-radius:4px;
    cursor: pointer; 
    cursor: hand;
}

.amp_scrollable img:hover {
  background-color:#000;
  border:1px solid #000;
}

/* active item */
.amp_scrollable .active {
    border:2px solid #000;
    position:relative;
    cursor:default;
}


#modalTabs { 
    background: transparent; 
    border: none; 
    padding: 0px;
    margin: 0px;
    width:616px;
} 

#modalTabs .ui-widget-header { 
    background: transparent; 
    border: none; 
    border-bottom: 1px solid #c0c0c0; 
    -moz-border-radius: 0px; 
    -webkit-border-radius: 0px; 
    border-radius: 0px; 
    padding: 0px;
    margin: 0px;
} 

#modalTabs .ui-tabs-nav .ui-state-default { 
    background: transparent; 
    border: none; 
} 

#modalTabs .ui-tabs-nav .ui-state-active { 
    /*background: transparent url(img/uiTabsArrow.png) no-repeat bottom center; */
    border: none; 
} 

#modalTabs .ui-tabs-nav .ui-state-default a { 
    color: #c0c0c0; 
} 

#modalTabs .ui-tabs-nav .ui-state-active a { 
    color: #000; 
}

#modalTabs .ui-tabs-nav li { 
    padding:0px;
    margin: 0px;
}

