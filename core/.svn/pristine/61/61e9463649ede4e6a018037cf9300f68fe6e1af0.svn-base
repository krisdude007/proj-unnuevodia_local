.dtStyle{
    border-collapse:collapse;
    width:100%;
}
th {
    padding:3px;
    border:1px solid black;
    background-color:#fff;
}

td {
    padding:3px;
    border:1px solid black;
    color:#FFF;
/*    background-color:#222222;*/
}
.grey{color:#999}

.row{
    margin-left:0px;
}
.dtStyle tr:nth-child(odd){
    background-color:#222222;
}

.dtStyle tr:nth-child(even){
    background-color:#323232;
}
