.btn-rotate {

  background-color: hsl(201, 100%, 30%) !important;
  background-repeat: repeat-x;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#00a5ff", endColorstr="#006399");
  background-image: -khtml-gradient(linear, left top, left bottom, from(#00a5ff), to(#006399));
  background-image: -moz-linear-gradient(top, #00a5ff, #006399);
  background-image: -ms-linear-gradient(top, #00a5ff, #006399);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #00a5ff), color-stop(100%, #006399));
  background-image: -webkit-linear-gradient(top, #00a5ff, #006399);
  background-image: -o-linear-gradient(top, #00a5ff, #006399);
  background-image: linear-gradient(#00a5ff, #006399);
  border-color: #006399 #006399 hsl(201, 100%, 25%);
  color: #fff !important;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.33);
  -webkit-font-smoothing: antialiased;
  padding: 5px;
}

#modalImageContainer {
    width: 640px;
    height: 360px;
    background: #000;
    text-align: center;
    vertical-align: middle;
}

#modalImage {
    max-width:640px;
    max-height:360px;
    width: auto;
    height: auto;
}


#modalTabs { 
    background: transparent; 
    border: none; 
    padding: 0px;
    margin: 0px;
    width: 550px;
} 

#modalTabs .ui-widget-header { 
    background: transparent; 
    border: none; 
    border-bottom: 1px solid #c0c0c0; 
    -moz-border-radius: 0px; 
    -webkit-border-radius: 0px; 
    border-radius: 0px; 
    padding: 0px;
    margin: 0px;
} 

#modalTabs .ui-tabs-nav .ui-state-default { 
    background: transparent; 
    border: none; 
} 

#modalTabs .ui-tabs-nav .ui-state-active { 
    /*background: transparent url(img/uiTabsArrow.png) no-repeat bottom center; */
    border: none; 
} 

#modalTabs .ui-tabs-nav .ui-state-default a { 
    color: #c0c0c0; 
} 

#modalTabs .ui-tabs-nav .ui-state-active a { 
    color: #000; 
}

#modalTabs .ui-tabs-nav li { 
    padding:0px;
    margin: 0px;
}

#tab-share {
    color: #fff;
}

