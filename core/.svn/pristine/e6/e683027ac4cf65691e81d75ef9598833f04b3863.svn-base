body {
    /* width: 100%;
    height: 100%;
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 0px 0px;
   overflow: hidden;*/
    background: rgb(0, 0, 0);
    background: rgba(0, 0, 0, 0.0);
    font-family: Verdana, Arial;
}

.message {
    position: absolute;
    top: calc(50% - 50px);
    width: 100%;
    margin: 0px 0px 0px 0px;
    text-align: center;
    color: #FFFFFF;
    font-size: 25pt;
    font-style: bold;
}

#sliderContainer {
    position: absolute; 
    width:100%;
    height: 70px;
    bottom: 0px;
}

.sliderKOORA {
    background: -webkit-linear-gradient(#f6aa27, #000000); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#f6aa27, #000000); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#f6aa27, #000000); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#f6aa27, #000000); /* Standard syntax */
     filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#f6aa27, endColorstr=#000000);
        /* For Internet Explorer 8 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#f6aa27, endColorstr=#000000)";
}

.sliderYAHALA {
    background: -webkit-linear-gradient(#00c6ff, #fa00ff); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#00c6ff, #fa00ff); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#00c6ff, #fa00ff); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#00c6ff, #fa00ff); /* Standard syntax */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#00c6ff, endColorstr=#fa00ff);
        /* For Internet Explorer 8 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#00c6ff, endColorstr=#fa00ff)";
}

.sliderFEESAMEEM {
    background: -webkit-linear-gradient(#ffffff, #fa7271); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#ffffff, #fa7271); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#ffffff, #fa7271); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#ffffff, #fa7271); /* Standard syntax */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#00c6ff, endColorstr=#fa00ff);
        /* For Internet Explorer 8 */
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorstr=#00c6ff, endColorstr=#fa00ff)";
}

.sliderALRISALA {
    background: -webkit-linear-gradient(#42d0d9, #0e95a3); /* For Safari 5.1 to 6.0 */
    background: -o-linear-gradient(#42d0d9, #0e95a3); /* For Opera 11.1 to 12.0 */
    background: -moz-linear-gradient(#42d0d9, #0e95a3); /* For Firefox 3.6 to 15 */
    background: linear-gradient(#42d0d9, #0e95a3); /* Standard syntax */
}


#sliderContainer .slidersRev {
    position: relative;
    height: 50px;
    margin: 10px 0px 0px 0px;
}

#sliderContainer .slidersRev .sliderRev {
    float: right;
    height: 50px;
    margin: 0px 50px 0px 0px;
    background-color: rgba(0, 0, 0, 0.0);
}

#sliderContainer .sliders {
    position: relative;
    height: 50px;
    margin: 10px 0px 0px 0px;
}

#sliderContainer .sliders .slider {
    float: right;
    height: 50px;
    margin: 0px 50px 0px 0px;
    background: rgba(0, 0, 0, 0.0);
}

.votegraphBG{
    width:480px;
    display: inline-block;
    /*float:right;*/
    margin-top:5px;
    background-color: #ededed;
    border: 1px solid #D6D5D3;
}

.voteLabels{
    margin-top:10px;
    font-size:20px;
    text-transform:uppercase;
}
.voteButtons
{
    width:147px;
}

.vote_footer
{font-size:15px; background-color:#000000; color:#000000; height: 37px; padding-top:9px;}

.vote_buttons
{
    margin-top:20px;
}
.vote_div
{
    background-color: rgba(0, 0, 0, 0.0);
/*  margin-right:20px;*/
    padding-top:20px;
    font-size:12px;
}

.vote_div .progress
{
    background-color:#ffffff;
    height:6px;
    overflow:none;
    margin-left:-1px;
    width:100% !important;
    border-radius:0px;
}

.vote_div .vote_label
{
     background-color: rgba(0, 0, 0, 0.0);
     margin-bottom:0px;
     border:none;
     height:20px;
     width:100%;
     color:#ffffff;
     font-size:12px !important;
}

.vote_div .progress-bar-info
{
    background-color:#1793d0;
}

.linkButton {
    border:0px;
    width:152px;
    height: 23px;
    background-color: #000;
    display:inline-block;
    color:rgba(0, 0, 0, 0.0);
    font-weight: 100;
    padding:6px 10px;
    margin: 5px;
    text-decoration:none;
    text-align: center;
    /*text-shadow:1px 1px 0px #528ecc;*/
}
.linkButton:hover {

}
.linkButton:active {
    position:relative;
    top:1px;
}
.linkButton:visited {
    position:relative;
    top:1px;
}

#sliderContainer .sliders .slider .avatar {
    float: left;
    width: 50px;
    height: 50px;
    margin: 0px 0px 0px 0px;
}

#sliderContainer .sliders .slider .avatar img {
    width: 50px;
    height: 50px;
    margin: 0px 0px 0px 0px;
}

#sliderContainer .sliders .slider .text {
    float: left;
    height: 50px;
    margin: 10px 10px 0px 10px;
    text-align: left;
    text-height: 100%; 
}

#sliderContainer .slidersRev .sliderRev .avatar {
    float: left;
    width: 50px;
    height: 50px;
    margin: 0px 0px 0px 0px;
}

#sliderContainer .slidersRev .sliderRev .avatar img {
    width: 50px;
    height: 50px;
    margin: 0px 0px 0px 0px;
}

#sliderContainer .slidersRev .sliderRev .text {
    float: left;
    height: 50px;
    margin: 0px 0px 0px 0px;
    text-align: left;
    text-height: 100%; 
}

